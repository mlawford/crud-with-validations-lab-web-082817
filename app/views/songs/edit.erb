<%= form_for @song do |f| %>
<label>My Song Name></label>
  <%= f.text_field :title %>
  <label>Release></label>
  <%= f.check_box :released %>
  <label>Release Year></label>
  <%= f.number_field :release_year %>
  <label>Genre></label>
  <%= f.text_field :genre %>
  <label>Artist></label>
  <%= f.text_field :artist_name %>

  <%= f.submit "Updates Song" %>
<%end%>
